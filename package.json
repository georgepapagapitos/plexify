{
  "name": "plexify",
  "version": "1.0.0",
  "description": "Plex media manager",
  "scripts": {
    "build": "tailwindcss -i ./static/src/input.css -o ./static/css/styles.css",
    "watch": "tailwindcss -i ./static/src/input.css -o ./static/css/styles.css --watch",
    "dev": "concurrently \"python manage.py runserver\" \"npm run watch\""
  },
  "dependencies": {},
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "concurrently": "^8.2.2"
  }
}